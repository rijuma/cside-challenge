{
  "name": ".",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3000 \" \"pnpm relay --watch\"",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "relay": "mkdirp ./src/utils/relay/__generated__ && relay-compiler"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-router": "^1.120.13",
    "@tanstack/react-router-devtools": "^1.120.13",
    "@tanstack/router-plugin": "^1.120.13",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "relay-runtime": "^18.2.0",
    "tailwindcss": "^4.1.8",
    "vite-plugin-relay": "^2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^18.2.5",
    "@vitejs/plugin-react": "^4.5.0",
    "concurrently": "^9.1.2",
    "jsdom": "^26.1.0",
    "mkdirp": "^3.0.1",
    "react-relay": "^19.0.0",
    "relay-compiler": "^18.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.0",
    "web-vitals": "^4.2.4"
  },
  "relay": {
    "src": "./src/",
    "schema": "./github.graphql",
    "language": "typescript",
    "excludes": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "artifactDirectory": "./src/utils/relay/__generated__",
    "noFutureProofEnums": true,
    "eagerEsModules": true
  },
  "packageManager": "pnpm@10.11.0"
}
